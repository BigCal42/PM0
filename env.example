# Demo mode (recommended for local development)
# Set to true to use local demo data without Supabase
VITE_USE_DEMO_DATA=true

# Supabase Configuration (required when VITE_USE_DEMO_DATA=false)
# VITE_SUPABASE_URL=https://your-project.supabase.co
# VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Optional: Sentry error tracking
# VITE_SENTRY_DSN=https://your-sentry-dsn


# Vercel MCP (Cursor) configuration
# VERCEL_ACCESS_TOKEN=your-vercel-token
# VERCEL_ORG_ID=your-vercel-org-id
# VERCEL_PROJECT_ID=your-vercel-project-id
