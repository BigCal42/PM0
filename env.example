# Demo mode (recommended for local development)
# Set to true to use local demo data without Supabase
VITE_USE_DEMO_DATA=true

# Supabase Configuration (required when VITE_USE_DEMO_DATA=false)
# VITE_SUPABASE_URL=https://your-project.supabase.co
# VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Optional: Sentry error tracking
# VITE_SENTRY_DSN=https://your-sentry-dsn

# PM0 Playground Mode (feature flag)
# Set to 1 to enable new UX surfaces: Home Hub, Project Canvas, Scenario Lab
# VITE_PM0_PLAYGROUND=1

# AI Guidance (optional, requires playground mode)
# Set to 1 to enable AI-powered insights and explanations
# VITE_USE_AI_GUIDANCE=1


# Vercel MCP (Cursor) configuration
# VERCEL_ACCESS_TOKEN=your-vercel-token
# VERCEL_ORG_ID=your-vercel-org-id
# VERCEL_PROJECT_ID=your-vercel-project-id
